{
  "name": "jsreport-public-templates",
  "version": "2.2.0",
  "description": "jsreport extension for granting public access to particular templates",
  "keywords": [
    "jsreport"
  ],
  "homepage": "https://github.com/jsreport/jsreport-public-templates",
  "bugs": {
    "url": "https://github.com/jsreport/jsreport-public-templates/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jsreport/jsreport-public-templates.git"
  },
  "license": "LGPL",
  "author": {
    "name": "Jan Blaha"
  },
  "files": [
    "lib",
    "studio",
    "index.js",
    "jsreport.config.js"
  ],
  "scripts": {
    "build": "jsreport-studio-build",
    "prepublish": "in-publish && jsreport-studio-build || not-in-publish",
    "start": "jsreport-studio-start --httpPort 5488",
    "test": "mocha test --timeout=5000"
  },
  "dependencies": {
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "babel-eslint": "9.0.0",
    "in-publish": "2.0.1",
    "jsreport-authentication": "2.6.1",
    "jsreport-authorization": "2.4.0",
    "jsreport-core": "2.10.1",
    "jsreport-express": "2.8.1",
    "jsreport-studio-dev": "1.6.0",
    "mocha": "5.2.0",
    "should": "13.2.3",
    "standard": "12.0.1",
    "supertest": "3.4.2"
  },
  "engines": {
    "node": ">=8.9"
  },
  "standard": {
    "ignore": [
      "studio/main.js"
    ],
    "parser": "babel-eslint",
    "env": {
      "node": true,
      "mocha": true,
      "browser": true
    }
  }
}
