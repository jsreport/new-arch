{
  "name": "jsreport-chrome-pdf",
  "version": "1.10.0",
  "description": "jsreport recipe which is rendering pdf from html using headless chrome",
  "keywords": [
    "jsreport",
    "pdf"
  ],
  "homepage": "https://github.com/jsreport/jsreport-chrome-pdf",
  "repository": {
    "type": "git",
    "url": "git@github.com:jsreport/jsreport-chrome-pdf.git"
  },
  "license": "MIT",
  "author": "Jan Blaha",
  "files": [
    "lib",
    "studio",
    "index.js",
    "jsreport.config.js"
  ],
  "scripts": {
    "build": "jsreport-studio-build",
    "prepublish": "in-publish && jsreport-studio-build || not-in-publish",
    "run": "npm run build && jsreport-studio-start --httpPort 5488 --run-only",
    "start": "jsreport-studio-start --httpPort 5488",
    "test": "mocha test --timeout 15000 && standard"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-eslint": "10.0.1",
    "eslint": "5.16.0",
    "eslint-config-standard-jsx": "6.0.2",
    "eslint-plugin-babel": "5.3.1",
    "handlebars": "4.7.7",
    "in-publish": "2.0.1",
    "jsreport-core": "2.10.1",
    "jsreport-handlebars": "2.1.0",
    "jsreport-studio-dev": "1.6.0",
    "mocha": "6.1.4",
    "parse-pdf": "1.0.0",
    "ps-list": "7.2.0",
    "puppeteer": "9.1.1",
    "should": "13.2.3",
    "standard": "12.0.1"
  },
  "peerDependencies": {
    "puppeteer": ">=9.1.x"
  },
  "engines": {
    "node": ">=8.9"
  },
  "standard": {
    "ignore": [
      "studio/main.js"
    ],
    "parser": "babel-eslint",
    "env": {
      "node": true,
      "mocha": true,
      "browser": true
    }
  }
}
