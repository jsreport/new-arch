{
  "name": "jsreport-pdf-utils",
  "version": "1.11.0",
  "description": "jsreport extension providing pdf operations like merge or concatenation",
  "keywords": [
    "jsreport",
    "pdf"
  ],
  "homepage": "https://github.com/jsreport/jsreport-pdf-utils",
  "bugs": {
    "url": "https://github.com/jsreport/jsreport-pdf-utils/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jsreport/jsreport-pdf-utils.git"
  },
  "license": "MIT",
  "author": {
    "name": "Jan Blaha"
  },
  "main": "index.js",
  "files": [
    "lib",
    "studio",
    "index.js",
    "jsreport.config.js"
  ],
  "scripts": {
    "build": "jsreport-studio-build",
    "lint": "standard",
    "prepublishOnly": "jsreport-studio-build",
    "start": "jsreport-studio-start --httpPort 5488",
    "test": "set DEBUG=jsreport&& mocha test --timeout=20000 && standard",
    "test-full": "set DEBUG=jsreport&&set TEST_FULL=true&& mocha test --timeout=20000 && standard"
  },
  "dependencies": {
    "cheerio": "0.22.0",
    "jsreport-pdfjs": "0.5.3",
    "lodash.omit": "4.5.0",
    "nanoid": "3.1.12",
    "node-forge": "0.10.0",
    "node-signpdf": "1.3.2",
    "node.extend": "2.0.2",
    "parse-color": "1.0.0",
    "pdfjs-dist": "2.0.489"
  },
  "devDependencies": {
    "babel-eslint": "10.1.0",
    "handlebars": "4.7.7",
    "jsreport-assets": "1.7.0",
    "jsreport-child-templates": "1.4.0",
    "jsreport-chrome-pdf": "1.10.0",
    "jsreport-core": "2.10.1",
    "jsreport-handlebars": "2.1.0",
    "jsreport-jsrender": "2.1.1",
    "jsreport-scripts": "2.6.0",
    "jsreport-studio-dev": "1.6.0",
    "mocha": "5.2.0",
    "puppeteer": "9.1.1",
    "rimraf": "2.6.3",
    "should": "13.2.3",
    "standard": "16.0.3"
  },
  "engines": {
    "node": ">=8.9"
  },
  "standard": {
    "ignore": [
      "studio/main.js"
    ],
    "parser": "babel-eslint",
    "env": {
      "node": true,
      "mocha": true,
      "browser": true
    }
  }
}
