{
  "name": "jsreport-version-control",
  "version": "1.6.0",
  "description": "jsreport extension adding support for versioning templates and other entities",
  "main": "index.js",
  "scripts": {
    "test": "set DEBUG=jsreport && mocha test --timeout 5000 && standard",
    "start": "jsreport-studio-start --httpPort 5488",
    "build": "jsreport-studio-build",
    "prepublish": "in-publish && jsreport-studio-build || not-in-publish"
  },
  "keywords": [
    "jsreport",
    "pdf"
  ],
  "standard": {
    "ignore": [
      "main.js"
    ],
    "parser": "babel-eslint",
    "env": {
      "node": true,
      "mocha": true,
      "browser": true
    }
  },
  "engines": {
    "node": ">=14.x.x"
  },
  "homepage": "https://github.com/jsreport/jsreport-version-control",
  "repository": {
    "type": "git",
    "url": "git@github.com:jsreport/jsreport-version-control.git"
  },
  "license": "MIT",
  "author": "Jan Blaha",
  "devDependencies": {
    "babel-eslint": "10.0.1",
    "eslint": "5.11.0",
    "eslint-config-standard-jsx": "6.0.2",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-standard": "4.0.1",
    "in-publish": "2.0.1",
    "jsreport-assets": "file:../jsreport-assets",
    "jsreport-authentication": "file:../jsreport-authentication",
    "jsreport-chrome-pdf": "file:../jsreport-chrome-pdf",
    "jsreport-core": "file:../jsreport-core",
    "jsreport-data": "file:../jsreport-data",
    "jsreport-express": "file:../jsreport-express",
    "jsreport-studio-dev": "file:../jsreport-studio-dev",
    "jsreport-templates": "file:../jsreport-templates",
    "mocha": "5.2.0",
    "puppeteer": "2.0.0",
    "rimraf": "2.6.3",
    "should": "13.2.3",
    "standard": "12.0.1",
    "supertest": "3.4.2"
  },
  "dependencies": {
    "bluebird": "3.5.4",
    "bytes": "3.0.0",
    "diff": "3.5.0",
    "diff-match-patch": "1.0.5",
    "diff2html": "2.5.0",
    "isbinaryfile": "3.0.3",
    "lodash.omit": "4.5.0",
    "mime-types": "2.1.27",
    "node.extend.without.arrays": "1.1.6"
  },
  "files": [
    "lib",
    "studio",
    "test",
    "static",
    "index.js",
    "jsreport.config.js"
  ]
}
